{
  "name": "Libisky",
  "version": "0.7.0",
  "main": "main.js",
  "build": {
    "appId": "com.libisky.app",
    "mac": {
      "icon": "icon.icns",
      "target": ["dmg","zip"]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": ["nsis","zip"]
    }
  },
  "scripts": {
    "dist": "electron-builder --mac --x64",
    "win": "electron-builder --win --x64"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "async": "^0.9.0",
    "iconv-lite": "^0.4.15",
    "nedb": "^1.1.1",
    "needle": "^0.7.10",
    "resanitize": "^0.3.0",
    "xml2js": "^0.4.6"
  },
  "devDependencies": {
    "electron-builder": "^22.9.1"
  }
}
